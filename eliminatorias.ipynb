   "id": "e41d7ca0-411f-444a-acdc-c508af2c71e5",
   "metadata": {},
   "outputs": [],
   "source": [
    "posiciones['Equipo'] = posiciones.Equipo.apply(lambda x : (x[:(len(x)//2)]))\n",
    "posiciones.drop(['Pos', 'PJ', 'G', 'E', 'P', 'GF', 'GC'], axis=1, inplace=True)"
   ]
